local targetPlayerName = ""
local cameraConnection = nil
local cameraOffset = CFrame.new(0, 3, -8) -- РљР°РјРµСЂР° СЃР·Р°РґРё Рё РЅРµРјРЅРѕРіРѕ СЃРІРµСЂС…Сѓ

-- Р¤СѓРЅРєС†РёСЏ РґР»СЏ РѕР±РЅРѕРІР»РµРЅРёСЏ РєР°РјРµСЂС‹
local function updateThirdPersonCamera()
    if not targetPlayerName or targetPlayerName == "" then return end
    
    local target = game.Players:FindFirstChild(targetPlayerName)
    if target and target.Character then
        local root = target.Character:FindFirstChild("HumanoidRootPart")
        if root then
            local camera = game.Workspace.CurrentCamera
            camera.CameraType = Enum.CameraType.Scriptable
            
            -- РљР°РјРµСЂР° СЃР»РµРґСѓРµС‚ Р·Р° РёРіСЂРѕРєРѕРј, РЅРѕ РѕСЃС‚Р°РµС‚СЃСЏ РїРѕР·Р°РґРё РЅРµРіРѕ
            local targetCFrame = root.CFrame
            camera.CFrame = targetCFrame * cameraOffset
        end
    end
end

local Section5 = Tabs.Main:AddSection("Spy")

local Input2 = Tabs.Main:AddInput("Input", {
    Title = "Spy Player",
    Placeholder = "Type Here!",
    Numeric = false, -- Only allows numbers
    Finished = true, -- Only calls callback when you press enter
    Callback = function(Value)
        -- РћС‚РєР»СЋС‡Р°РµРј РїСЂРµРґС‹РґСѓС‰РµРµ СЃРѕРµРґРёРЅРµРЅРёРµ
    if cameraConnection then
        cameraConnection:Disconnect()
        cameraConnection = nil
    end
    
    targetPlayerName = text
    
    -- Р’РєР»СЋС‡Р°РµРј СЃР»РµР¶РµРЅРёРµ, РµСЃР»Рё РІРІРµРґРµРЅРѕ РёРјСЏ
    if text ~= "" then
        cameraConnection = game:GetService("RunService").RenderStepped:Connect(updateThirdPersonCamera)
    else
        -- Р’РѕР·РІСЂР°С‰Р°РµРј РєР°РјРµСЂСѓ РІ СЃС‚Р°РЅРґР°СЂС‚РЅС‹Р№ СЂРµР¶РёРј
        game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
    end
end
})

Tabs.Main:AddButton({
    Title = "Stop Spy Player",
    Callback = function()
     if cameraConnection then
        cameraConnection:Disconnect()
        cameraConnection = nil
     end
     game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
     targetPlayerName = ""   
    end
})

