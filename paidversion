local Section3 = Tabs.Main:AddSection("Kill Stuff")

local autoEquip = false
local connection = nil

local punchActive = false
local punchConnection = nil

local Toggle6 = Tabs.Main:AddToggle("MyToggle", 
{
    Title = "Auto Normal Punch", 
    Default = false,
    Callback = function(state)
	punchActive = state
    
    -- лалѕламЉла┬░ла┬╗лАлЈла┬хлаЛў лаЛЌлАлѓла┬хламЉлАРђ╣ламЉлАЛЊлАРђ░ла┬хла┬х лаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х, ла┬хлАлЃла┬╗лаЛЉ ла┬▒лАРђ╣ла┬╗лаЛЋ
    if punchConnection then
        punchConnection:Disconnect()
        punchConnection = nil
    end
    
    if state then
        local player = game.Players.LocalPlayer
        
        local function startPunch()
            local char = player.Character or player.CharacterAdded:Wait()
            
            while punchActive and char and char:IsDescendantOf(workspace) do
                local punchTool = player.Backpack:FindFirstChild("Punch") or char:FindFirstChild("Punch")
                
                if punchTool then
                    -- лаЛњлалєлАРђџлаЛЋ-лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћла┬░
                    if punchTool.Parent ~= char then
                        char:WaitForChild("Humanoid"):EquipTool(punchTool)
                        task.wait(0.1) -- лаРђЮла┬░ла┬хлаЛў лалєлАлѓла┬хлаЛўлАлЈ лалЁла┬░ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлАЛЊ
                    end
                    
                    -- лаЛњлаЛћлАРђџлаЛЉлалєла┬░лАРђалаЛЉлАлЈ лАЛЊламЉла┬░лАлѓла┬░ лАлЃ лаЛЋла┬▒лАРђ╣лАРђАлалЁлаЛЋлаРёќ лАлЃлаЛћлаЛЋлАлѓлаЛЋлАлЃлАРђџлАлілАлІ
                    punchTool:Activate()
                end
                task.wait(0.05) -- ла┬ўлалЁлАРђџла┬хлАлѓлалєла┬░ла┬╗ лаЛўла┬хла┬ХламЉлАЛЊ лАЛЊламЉла┬░лАлѓла┬░лаЛўлаЛЉ (лаЛўлаЛЋла┬ХлалЁлаЛЋ лАлѓла┬хлаЛќлАЛЊла┬╗лаЛЉлАлѓлаЛЋлалєла┬░лАРђџлАлі)
            end
        end
        
        -- лаРђћла┬░лаЛЌлАЛЊлАлЃлаЛћла┬░ла┬хлаЛў лАлЃлАлѓла┬░ла┬илАЛЊ лаЛЉ лаЛЌлАлѓлаЛЉ лаЛћла┬░ла┬ХламЉлаЛЋлаЛў лАлѓла┬хлАлЃлаЛЌла┬░лалєлалЁла┬х
        punchConnection = player.CharacterAdded:Connect(function()
            if punchActive then
                task.wait(0.5) -- лаРђЮла┬░ла┬хлаЛў лалєлАлѓла┬хлаЛўлАлЈ лалЁла┬░ лаЛЌлаЛЋлАлЈлалєла┬╗ла┬хлалЁлаЛЉла┬х лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
                startPunch()
            end
        end)
        
        startPunch() -- лаЛЪла┬хлАлѓлалєлАРђ╣лаРёќ ла┬ила┬░лаЛЌлАЛЊлАлЃлаЛћ
    end 
end
})

-- ла┬цлАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ ламЉла┬╗лАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ
local function attackPlayers()
    local player = game.Players.LocalPlayer
    
    -- лаЛюла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░ ла┬▒лАРђ╣лАлЃлАРђџлАлѓлаЛЋлаРёќ ла┬░лАРђџла┬░лаЛћлаЛЉ
    local punch = player.Backpack:FindFirstChild("Punch") or (player.Character and player.Character:FindFirstChild("Punch"))
    if punch and punch:FindFirstChild("attackTime") then
        punch.attackTime.Value = 0.05
    end
    
    -- лаЛњлАРђџла┬░лаЛћла┬░ лалєлАлЃла┬хлАРђд лаЛЉлаЛќлАлѓлаЛЋлаЛћлаЛЋлалє
    for _, target in ipairs(game.Players:GetPlayers()) do
        if target ~= player then
            local root = target.Character and target.Character:FindFirstChild("HumanoidRootPart")
            local rHand = player.Character and player.Character:FindFirstChild("RightHand")
            local lHand = player.Character and player.Character:FindFirstChild("LeftHand")
            
 if root and rHand and lHand then
                firetouchinterest(rHand, root, 1)
                firetouchinterest(lHand, root, 1)
                firetouchinterest(rHand, root, 0)
                firetouchinterest(lHand, root, 0)
            end
        end
    end
end

local autoKill = false
local connection = nil

local Toggle7 = Tabs.Main:AddToggle("MyToggle", 
{
    Title = "Auto Kill", 
    Default = false,
    Callback = function(state)
	autoKill = state
    
    if connection then
        connection:Disconnect()
        connection = nil
    end
    
    if state then
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁлаЛЋлаРёќ лАРђалаЛЉлаЛћла┬╗ ла┬░лАРђџла┬░лаЛћлаЛЉ
        connection = game:GetService("RunService").Heartbeat:Connect(attackPlayers)
        
        -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
        game.Players.LocalPlayer.CharacterAdded:Connect(function()
            if autoKill then
                task.wait(0.5) -- лаЛюла┬хла┬▒лаЛЋла┬╗лАлілАРѓгла┬░лАлЈ ла┬ила┬░ламЉла┬хлАлѓла┬ХлаЛћла┬░ лаЛЌлаЛЋлАлЃла┬╗ла┬х лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
                attackPlayers()
            end
        end)
    end
end
})

