local Section2 = Tabs.Main:AddSection("Rebirths Folder")

local toggleLoop = nil
local Toggle1 = Tabs.Main:AddToggle("MyToggle", 
{
    Title = "Auto Lift", 
    Default = false,
    Callback = function(state)
	if state then
        toggleLoop = task.spawn(function()
            while true do
                game.Players.LocalPlayer:WaitForChild("muscleEvent"):FireServer("rep")
                task.wait(0.1)
            end
        end)
    else
        if toggleLoop then
            task.cancel(toggleLoop)  -- лаЛЏлАлЃлАРђџла┬░лалЁла┬░лалєла┬╗лаЛЉлалєла┬░ла┬хлаЛў лАРђалаЛЉлаЛћла┬╗
            toggleLoop = nil
        end
    end
end
})

local Toggle2 = Tabs.Main:AddToggle("MyToggle", 
{
    Title = "Auto Normal Rebirth", 
    Default = false,
    Callback = function(state)
	autoRebirthEnabled = state
    
    if state then
        local player = game:GetService("Players").LocalPlayer
        local rebirthConnection
        local deathConnection

        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлѓла┬хла┬▒лаЛЉлАлѓлАРђџла┬░
        local function doRebirth()
            if autoRebirthEnabled then
                game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("rebirthRemote"):InvokeServer("rebirthRequest")
            end
        end

        -- ла┬длаЛЉлаЛћла┬╗ лАлѓла┬хла┬▒лаЛЉлАлѓлАРђџлаЛЋлалє (лАлЃ лаЛЉлалЁлАРђџла┬хлАлѓлалєла┬░ла┬╗лаЛЋлаЛў 0.1 лАлЃла┬хлаЛћ)
        rebirthConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if autoRebirthEnabled then
                doRebirth()
            end
        end)

        -- лаЛњлалєлАРђџлаЛЋ-лаЛЌла┬хлАлѓла┬хлаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х лаЛЌлаЛЋлАлЃла┬╗ла┬х лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ
        local function setupRespawn()
            if player.Character then
                deathConnection = player.Character.Humanoid.Died:Connect(function()
                    task.wait(0.5)
                    if autoRebirthEnabled then
                        rebirthConnection = game:GetService("RunService").Heartbeat:Connect(doRebirth)
                    end
                end)
            end
        end

        setupRespawn()
        player.CharacterAdded:Connect(setupRespawn)
        
    else
        if rebirthConnection then rebirthConnection:Disconnect() end
        if deathConnection then deathConnection:Disconnect() end
    end
end
})

local Toggle3 = Tabs.Main:AddToggle("MyToggle", 
{
    Title = "Auto Weight", 
    Default = false,
    Callback = function(state)
	autoWeightEnabled = state
    
    if state then
        
        local weightLoop
        local deathConnection
        local characterAddedConnection
        local player = game:GetService("Players").LocalPlayer
        
        -- лалѕла┬╗лАЛЊлАРђАлАРѓгла┬хлалЁлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлаЛЉ
        local function equipWeight()
            local character = player.Character
            if not character or not character:FindFirstChild("Humanoid") then return false end
            
            local humanoid = character:FindFirstChild("Humanoid")
            local weightTool = player.Backpack:FindFirstChild("Weight") or character:FindFirstChild("Weight")
            
            if weightTool and weightTool:IsA("Tool") then
                -- лаРђблАлЃла┬╗лаЛЉ Weight лалє ла┬▒лАлїлаЛћлаЛЌла┬░лаЛћла┬х - лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлАЛЊла┬хлаЛў
                if weightTool.Parent == player.Backpack then
                    humanoid:EquipTool(weightTool)
                    task.wait(0.2) -- лаРђЮла┬░ла┬хлаЛў лалєлАлѓла┬хлаЛўлАлЈ лалЁла┬░ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлАЛЊ
                end
                return true
            end
            return false
        end
        
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁлаЛЋлаРёќ лАРђалаЛЉлаЛћла┬╗ ла┬░лаЛћлАРђџлаЛЉлалєла┬░лАРђалаЛЉлаЛЉ
        local function activateWeight()
            while autoWeightEnabled and task.wait(0.2) do
                if player.Character and player.Character:FindFirstChild("Humanoid") then
                    -- лаЛЪлаЛЋлАлЃлАРђџлаЛЋлАлЈлалЁлалЁлаЛЋ лаЛЌлАРђ╣лАРђџла┬░ла┬хлаЛўлАлЃлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєла┬░лАРђџлАлі (лалЁла┬░ лАлЃла┬╗лАЛЊлАРђАла┬░лаРёќ, ла┬хлАлЃла┬╗лаЛЉ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџ лАЛЊлаЛЌла┬░ла┬╗)
                    equipWeight()
                    
                    local tool = player.Character:FindFirstChild("Weight")
                    if tool and tool:IsA("Tool") then
                        tool:Activate()
                    end
                end
            end
        end
        
        -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        local function onCharacterAdded(character)
            if deathConnection then deathConnection:Disconnect() end
            
            deathConnection = character:WaitForChild("Humanoid").Died:Connect(function()
                if weightLoop then task.cancel(weightLoop) end
                
                task.wait(1.5) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лАлѓла┬хлАлЃлаЛЌла┬░лалєлалЁла┬░
                
                if autoWeightEnabled then
                    -- лаРђЮлалєлаЛЋлаРёќлалЁла┬░лАлЈ лаЛЌлАлѓлаЛЋлалєла┬хлАлѓлаЛћла┬░ лаЛЌлаЛЋлАлЃла┬╗ла┬х лАлѓла┬хлАлЃлаЛЌла┬░лалєлалЁла┬░
                    task.wait(0.5)
                    equipWeight()
                    weightLoop = task.spawn(activateWeight)
                end
            end)
            
            if autoWeightEnabled then
                task.wait(1) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лаЛЌлаЛЋла┬╗лалЁлаЛЋлаРёќ ла┬ила┬░лаЛќлАлѓлАЛЊла┬илаЛћлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
                equipWeight()
                weightLoop = task.spawn(activateWeight)
            end
        end
        
        -- лаЛЪла┬хлАлѓлалєлаЛЋлалЁла┬░лАРђАла┬░ла┬╗лАлілалЁла┬░лАлЈ лалЁла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░
        if player.Character then
            onCharacterAdded(player.Character)
        end
        
        -- лаЛЪлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬░ла┬хлаЛў лаЛЋла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лалЁлаЛЋлалєлаЛЋлаЛќлаЛЋ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        characterAddedConnection = player.CharacterAdded:Connect(onCharacterAdded)
        
    else
        -- лаРђЮла┬хла┬░лаЛћлАРђџлаЛЉлалєла┬░лАРђалаЛЉлАлЈ
        if weightLoop then task.cancel(weightLoop) end
        if deathConnection then deathConnection:Disconnect() end
        if characterAddedConnection then characterAddedConnection:Disconnect() end
    end
end
})

local Toggle4 = Tabs.Main:AddToggle("MyToggle", 
{
    Title = "Auto Pushups", 
    Default = false,
    Callback = function(state)
        autoPushupsEnabled = state
    
        if state then
            local pushupsLoop
            local deathConnection
            local characterAddedConnection
            local player = game:GetService("Players").LocalPlayer
            
            -- Improved function for equipping pushup tool
            local function equipPushups()
                local character = player.Character
                if not character or not character:FindFirstChild("Humanoid") then return false end
                
                local humanoid = character:FindFirstChild("Humanoid")
                local pushupsTool = player.Backpack:FindFirstChild("Pushups") or character:FindFirstChild("Pushups")
                
                if pushupsTool and pushupsTool:IsA("Tool") then
                    -- If Pushups is in backpack - equip it
                    if pushupsTool.Parent == player.Backpack then
                        humanoid:EquipTool(pushupsTool)
                        task.wait(0.2) -- Give time for equipping
                    end
                    return true
                end
                return false
            end
            
            -- Main activation loop
            local function activatePushups()
                while autoPushupsEnabled and task.wait(0.2) do
                    if player.Character and player.Character:FindFirstChild("Humanoid") then
                        -- Constantly try to equip (in case tool dropped)
                        equipPushups()
                        
                        local tool = player.Character:FindFirstChild("Pushups")
                        if tool and tool:IsA("Tool") then
                            tool:Activate()
                        end
                    end
                end
            end
            
            -- Character death handler
            local function onCharacterAdded(character)
                if deathConnection then deathConnection:Disconnect() end
                
                deathConnection = character:WaitForChild("Humanoid").Died:Connect(function()
                    if pushupsLoop then task.cancel(pushupsLoop) end
                    
                    task.wait(1.5) -- Increased respawn time
                    
                    if autoPushupsEnabled then
                        -- Double check after respawn
                        task.wait(0.5)
                        equipPushups()
                        pushupsLoop = task.spawn(activatePushups)
                    end
                end)
                
                if autoPushupsEnabled then
                    task.wait(1) -- Increased time for full character load
                    equipPushups()
                    pushupsLoop = task.spawn(activatePushups)
                end
            end
            
            -- Initial setup
            if player.Character then
                onCharacterAdded(player.Character)
            end
            
            -- Connect new character handler
            characterAddedConnection = player.CharacterAdded:Connect(onCharacterAdded)
            
        else
            -- Deactivation
            if pushupsLoop then task.cancel(pushupsLoop) end
            if deathConnection then deathConnection:Disconnect() end
            if characterAddedConnection then characterAddedConnection:Disconnect() end
        end
    end
})

local Toggle5 = Tabs.Main:AddToggle("MyToggle", 
{
    Title = "Auto Size 2", 
    Default = false,
    Callback = function(state)
	autoSizeEnabled = state
    
    if state then
        
        local player = game.Players.LocalPlayer
        local sizeLoop
        local deathConnection

        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁлаЛЋлаРёќ лАРђалаЛЉлаЛћла┬╗ лаЛЉла┬илаЛўла┬хлалЁла┬хлалЁлаЛЉлАлЈ лАлѓла┬░ла┬илаЛўла┬хлАлѓла┬░
        local function changeSize()
            while autoSizeEnabled do
                game:GetService("ReplicatedStorage").rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", 2)
                task.wait()
            end
        end

        -- лаЛњлалєлАРђџлаЛЋ-лаЛЌла┬хлАлѓла┬хлаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х лаЛЌлаЛЋлАлЃла┬╗ла┬х лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ
        local function setupRespawn()
            if player.Character then
                deathConnection = player.Character.Humanoid.Died:Connect(function()
                    if sizeLoop then task.cancel(sizeLoop) end
                    task.wait(0.5)
                    if autoSizeEnabled then
                        sizeLoop = task.spawn(changeSize)
                    end
                end)
            end
        end

        -- лалјлАРђџла┬░лАлѓлАРђџ лаЛЋлАлЃлалЁлаЛЋлалєлалЁлаЛЋлаЛќлаЛЋ лАРђалаЛЉлаЛћла┬╗ла┬░
        sizeLoop = task.spawn(changeSize)
        setupRespawn()
        player.CharacterAdded:Connect(setupRespawn)
        
    else
        if sizeLoop then task.cancel(sizeLoop) end
        if deathConnection then deathConnection:Disconnect() end
    end
end
})

local targetCFrame = CFrame.new(-8747.04199, 121.162392, -5858.64014, 0.66414386, -4.84184497e-08, 0.747604787, 7.34292414e-08, 1, -4.67000827e-10, -0.747604787, 5.52062112e-08, 0.66414386)
local teleportActive = false
local teleportThread

local function teleportPlayer()
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetCFrame
    end
end

local function toggleTeleport()
    teleportActive = not teleportActive
    
    if teleportActive then
        -- лаРђћла┬░лаЛЌлАЛЊлАлЃлаЛћла┬░ла┬хлаЛў лАРђџла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлІ
        teleportThread = coroutine.create(function()
            while teleportActive do
                teleportPlayer()
                wait(1)
            end
        end)
        coroutine.resume(teleportThread)
        
        -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
            if teleportActive then
                character:WaitForChild("HumanoidRootPart")
                teleportPlayer()
            end
        end)
    else
        -- лаЛЏлАлЃлАРђџла┬░лалЁла┬░лалєла┬╗лаЛЉлалєла┬░ла┬хлаЛў лАРђџла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлІ
        if teleportThread then
            coroutine.close(teleportThread)
        end
    end
end

Tabs.Main:AddButton({
    Title = "Auto King",
    Callback = function()
     toggleTeleport() 
    end
})

-- лаЛЪла┬хлАлѓла┬хлаЛўла┬хлалЁлалЁлАРђ╣ла┬х
local currentRebirthGoal = 0
local autoCheckEnabled = false
local checkConnection = nil

-- ла┬цлАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лаЛЌлАлѓлаЛЋлалєла┬хлАлѓлаЛћлаЛЉ
local function checkRebirths()
    if not autoCheckEnabled then return end
    
    local currentRebirths = game:GetService("Players").LocalPlayer.leaderstats.Rebirths.Value
    
    if currentRebirths >= currentRebirthGoal then
        print("ла┬дла┬хла┬╗лАлі ламЉлаЛЋлАлЃлАРђџлаЛЉлаЛќлалЁлАЛЊлАРђџла┬░! лаЛъла┬хлаЛћлАЛЊлАРђ░лаЛЉла┬х лАлѓла┬хла┬▒лаЛЉлАлѓлАРђџлАРђ╣:", currentRebirths)
        if checkConnection then
            checkConnection:Disconnect()
        end
        if Toggle2 then -- лаЛЪлАлѓлаЛЋлалєла┬хлАлѓлАлЈла┬хлаЛў, лАлЃлАЛЊлАРђ░ла┬хлАлЃлАРђџлалєлАЛЊла┬хлАРђџ ла┬╗лаЛЉ Toggle2
            Toggle2:SetValue(false) -- лаРђЎлАРђ╣лаЛћла┬╗лАлІлАРђАла┬░ла┬хлаЛў лаЛЌлАлѓлаЛЋлалєла┬хлАлѓлаЛћлАЛЊ
        checkAndDisableToggle()
        end
    else
        print("лаЛЪлАлѓлаЛЋлаЛќлАлѓла┬хлАлЃлАлЃ:", currentRebirths.."/"..currentRebirthGoal)
    end
end

local Toggle27 = Tabs.Main:AddToggle("MyToggle", {
    Title = "Use Rebirth Goal", 
    Default = false,
    Callback = function(state)
        autoCheckEnabled = state
        
        if state then
            -- лаРђЎлаЛћла┬╗лАлІлАРђАла┬░ла┬хлаЛў лаЛЌлАлѓлаЛЋлалєла┬хлАлѓлаЛћлАЛЊ
            checkConnection = game:GetService("Players").LocalPlayer.leaderstats.Rebirths.Changed:Connect(function()
                checkRebirths()
            end)
            print("Auto-check ENABLED")
        else
            -- лаРђЎлАРђ╣лаЛћла┬╗лАлІлАРђАла┬░ла┬хлаЛў лаЛЌлАлѓлаЛЋлалєла┬хлАлѓлаЛћлАЛЊ
            if checkConnection then
                checkConnection:Disconnect()
            end
            print("Auto-check DISABLED")
        end
    end
})

local function checkAndDisableToggle()
    -- лаЛЪлАлѓлаЛЋлалєла┬хлАлѓлАлЈла┬хлаЛў лАлЃлаЛЋлАлЃлАРђџлаЛЋлАлЈлалЁлаЛЉла┬х
    if Toggle2.Value then
        print("Toggle2 ла┬▒лАРђ╣ла┬╗ лалєлаЛћла┬╗лАлІлАРђАла┬хлалЁ - лалєлАРђ╣лаЛћла┬╗лАлІлАРђАла┬░ла┬хлаЛў")
        Toggle2:Set(false)  -- лаРђЎлАРђ╣лаЛћла┬╗лАлІлАРђАла┬░ла┬хлаЛў
        Fluent:Notify({
            Title = "лалѕлалєла┬хламЉлаЛЋлаЛўла┬╗ла┬хлалЁлаЛЉла┬х",
            Content = "лаЛњлалєлАРђџлаЛЋ-лАРђъла┬хлАлѓлаЛўла┬░ ла┬▒лАРђ╣ла┬╗ла┬░ лалєлАРђ╣лаЛћла┬╗лАлІлАРђАла┬хлалЁла┬░",
            Duration = 3
        })
    else
        print("Toggle2 лАЛЊла┬Хла┬х лалєлАРђ╣лаЛћла┬╗лАлІлАРђАла┬хлалЁ")
    end
end

