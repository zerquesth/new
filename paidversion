local Section4 = Tabs.Main:AddSection("Target")

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- лАлЙлил┤л░лхл╝ Dropdown л┤л╗ЛЈ л▓ЛІл▒лЙЛђл░ Лєлхл╗лИ
local Dropdown = Tabs.Main:AddDropdown("TargetDropdown", {
    Title = "лњЛІл▒лхЛђлИЛѓлх Лєлхл╗Лї",
    Values = {},
    Multi = false,
    Default = nil,
})

local Toggle = Tabs.Main:AddToggle("TargetLockToggle", {
    Title = "лЌл░л║Лђлхл┐лИЛѓЛї Лєлхл╗Лї",
    Default = false,
    Callback = function(state)
        targetLockActive = state
        
        if state then
            local targetName = Dropdown.Value
            if not targetName then
                warn("Рџа№ИЈ лўл│ЛђлЙл║ лйлх л▓ЛІл▒Лђл░лй!")
                Toggle:SetValue(false)
                return
            end
            
            local targetPlayer = Players:FindFirstChild(targetName)
            if not targetPlayer then
                warn("­Ъџе лўл│ЛђлЙл║ лйлх лйл░л╣л┤лхлй!")
                Toggle:SetValue(false)
                return
            end
            
            -- лАлЙЛЁЛђл░лйЛЈлхл╝ Лєлхл╗Лї л▓ л│л╗лЙл▒л░л╗ЛїлйЛЃЛј л┐лхЛђлхл╝лхлйлйЛЃЛј
            _G.TargetPlayer = targetPlayer
            
            -- лъл▒Лђл░л▒лЙЛѓЛЄлИл║ л┐лЙЛЈл▓л╗лхлйлИЛЈ л┐лхЛђЛЂлЙлйл░лХл░ ЛЃ Лєлхл╗лИ
            _G.characterConnection = targetPlayer.CharacterAdded:Connect(function(character)
                _G.TargetCharacter = character
                warn("­Ъј» лЪлхЛђЛЂлЙлйл░лХ Лєлхл╗лИ лЙл▒лйлЙл▓л╗лхлй: "..targetName)
            end)
            
            -- лБЛЂЛѓл░лйл░л▓л╗лИл▓л░лхл╝ Лѓлхл║ЛЃЛЅлхл│лЙ л┐лхЛђЛЂлЙлйл░лХл░ л║л░л║ Лєлхл╗Лї (лхЛЂл╗лИ лхЛЂЛѓЛї)
            if targetPlayer.Character then
                _G.TargetCharacter = targetPlayer.Character
            end
            
            warn("­Ъћњ лдлхл╗Лї лил░л║Лђлхл┐л╗лхлйл░: "..targetName)
        else
            -- лъЛѓл║л╗ЛјЛЄл░лхл╝ лил░л║Лђлхл┐л╗лхлйлИлх
            if _G.characterConnection then 
                _G.characterConnection:Disconnect() 
            end
            _G.TargetPlayer = nil
            _G.TargetCharacter = nil
            warn("­ЪЪб лдлхл╗Лї ЛЂл▒ЛђлЙЛѕлхлйл░")
        end
    end
})

-- лъл▒лйлЙл▓л╗ЛЈлхл╝ ЛЂл┐лИЛЂлЙл║ лИл│ЛђлЙл║лЙл▓
local function updatePlayerList()
    local playerNames = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            table.insert(playerNames, player.Name)
        end
    end
    Dropdown:SetValues(playerNames)
local autoEquip = false
local punchActive = false
local punchConnection = nil
local autoKill = false
local killConnection = nil
local autoEquip = false
local punchActive = false
local punchConnection = nil
local autoKill = false
local killConnection = nil

-- лАлЙлил┤л░ЛЉл╝ Dropdown л┤л╗ЛЈ л▓ЛІл▒лЙЛђл░ лИл│ЛђлЙл║л░
local playerDropdown = Tabs.Main:AddDropdown("PlayerDropdown", {
    Title = "лњЛІл▒лхЛђлИЛѓлх лИл│ЛђлЙл║л░",
    Values = {}, -- лЌл░л┐лЙл╗лйлИЛѓЛЂЛЈ л┤лИлйл░л╝лИЛЄлхЛЂл║лИ
    Multi = false,
    Default = nil,
})

-- лъл▒лйлЙл▓л╗ЛЈлхл╝ ЛЂл┐лИЛЂлЙл║ лИл│ЛђлЙл║лЙл▓
local function updatePlayerList()
    local players = {}
    for _, player in ipairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer then
            table.insert(players, player.Name)
        end
    end
    playerDropdown:SetValues(players)
end

-- лўлйлИЛєлИл░л╗лИлил░ЛєлИЛЈ лИ лЙл▒лйлЙл▓л╗лхлйлИлх л┐ЛђлИ лИлил╝лхлйлхлйлИлИ
updatePlayerList()
game.Players.PlayerAdded:Connect(updatePlayerList)
game.Players.PlayerRemoving:Connect(updatePlayerList)

-- лцЛЃлйл║ЛєлИЛЈ л┤л╗ЛЈ л░Лѓл░л║лИ л║лЙлйл║ЛђлхЛѓлйлЙл│лЙ лИл│ЛђлЙл║л░
local function attackSelectedPlayer()
    local selectedPlayerName = playerDropdown.Value
    if not selectedPlayerName then return end
    
    local player = game.Players.LocalPlayer
    local target = game.Players:FindFirstChild(selectedPlayerName)
    
    if not target or not target.Character then return end
    
    local punch = player.Backpack:FindFirstChild("Punch") or (player.Character and player.Character:FindFirstChild("Punch"))
    if punch then
        -- лљл▓ЛѓлЙ-ЛЇл║лИл┐лИЛђлЙл▓л║л░
        if punch.Parent ~= player.Character then
            player.Character:WaitForChild("Humanoid"):EquipTool(punch)
            task.wait(0.1)
        end
        
        -- лЮл░ЛЂЛѓЛђлЙл╣л║л░ ЛЂл║лЙЛђлЙЛЂЛѓлИ л░Лѓл░л║лИ
        if punch:FindFirstChild("attackTime") then
            punch.attackTime.Value = 0.01
        end
        
        -- лљЛѓл░л║л░ л▓ЛІл▒Лђл░лйлйлЙл│лЙ лИл│ЛђлЙл║л░
        local root = target.Character:FindFirstChild("HumanoidRootPart")
        local rHand = player.Character:FindFirstChild("RightHand")
        local lHand = player.Character:FindFirstChild("LeftHand")
        
        if root and rHand and lHand then
            firetouchinterest(rHand, root, 1)
            firetouchinterest(lHand, root, 1)
            firetouchinterest(rHand, root, 0)
            firetouchinterest(lHand, root, 0)
        end
    end
end

-- лблЙл│л│л╗ л┤л╗ЛЈ л░л▓ЛѓлЙл╝л░ЛѓлИЛЄлхЛЂл║лЙл╣ л░Лѓл░л║лИ
local Toggle = Tabs.Main:AddToggle("AutoAttackToggle", {
    Title = "Auto Attack Selected Player",
    Default = false,
    Callback = function(state)
        punchActive = state
        autoKill = state
        
        -- лъЛЄлИЛЅл░лхл╝ л┐Лђлхл┤ЛІл┤ЛЃЛЅлИлх ЛЂлЙлхл┤лИлйлхлйлИЛЈ
        if punchConnection then
            punchConnection:Disconnect()
            punchConnection = nil
        end
        
        if killConnection then
            killConnection:Disconnect()
            killConnection = nil
        end
        
        if state then
            -- лъЛЂлйлЙл▓лйлЙл╣ ЛєлИл║л╗ л░Лѓл░л║лИ
            punchConnection = game:GetService("RunService").Heartbeat:Connect(function()
                attackSelectedPlayer()
            end)
            
            -- лъл▒Лђл░л▒лЙЛѓЛЄлИл║ ЛђлхЛЂл┐л░л▓лйл░
            killConnection = game.Players.LocalPlayer.CharacterAdded:Connect(function()
                if punchActive then
                    task.wait(0.5)
                    attackSelectedPlayer()
                end
            end)
        end
    end
})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

-- лАлЙлил┤л░лхл╝ Dropdown л┤л╗ЛЈ л▓ЛІл▒лЙЛђл░ лИл│ЛђлЙл║л░
local PlayerDropdown = Tabs.Main:AddDropdown("PlayerSpyDropdown", {
    Title = "лњЛІл▒лхЛђлИЛѓлх лИл│ЛђлЙл║л░ л┤л╗ЛЈ ЛЂл╗лхлХл║лИ",
    Values = {},
    Multi = false,
    Default = nil,
})

-- лАлЙлил┤л░лхл╝ Toggle л┤л╗ЛЈ л░л║ЛѓлИл▓л░ЛєлИлИ ЛЂл╗лхлХл║лИ
local SpyToggle = Tabs.Main:AddToggle("SpyPlayerToggle", {
    Title = "лАл╗лхл┤лИЛѓЛї лил░ лИл│ЛђлЙл║лЙл╝",
    Default = false,
    Callback = function(state)
        spyActive = state
        
        if state then
            local targetName = PlayerDropdown.Value
            if not targetName then
                warn("Рџа№ИЈ лАлйл░ЛЄл░л╗л░ л▓ЛІл▒лхЛђлИЛѓлх лИл│ЛђлЙл║л░!")
                SpyToggle:SetValue(false)
                return
            end
            
            local TargetPlayer = Players:FindFirstChild(targetName)
            if not TargetPlayer then
                warn("­Ъџе лўл│ЛђлЙл║ лйлх лйл░л╣л┤лхлй!")
                SpyToggle:SetValue(false)
                return
            end
            
            -- лцЛЃлйл║ЛєлИЛЈ ЛЂл╗лхлХлхлйлИЛЈ лил░ л┐лхЛђЛЂлЙлйл░лХлхл╝
            local function trackCharacter()
                if TargetPlayer.Character and TargetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    Camera.CameraType = Enum.CameraType.Scriptable
                    Camera.CFrame = CFrame.new(
                        TargetPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 3, -5),
                        TargetPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 2, 0)
                    )
                end
            end
            
            -- лЌл░л┐ЛЃЛЂл║л░лхл╝ ЛЂл╗лхлХлхлйлИлх
            spyConnection = game:GetService("RunService").Heartbeat:Connect(function()
                trackCharacter()
            end)
            
            -- лъл▒Лђл░л▒лЙЛѓЛЄлИл║ ЛЂл╝лхлйЛІ л┐лхЛђЛЂлЙлйл░лХл░
            characterConnection = TargetPlayer.CharacterAdded:Connect(function()
                task.wait(1) -- лќл┤лхл╝ л┐лЙл╗лйлЙл╣ лил░л│ЛђЛЃлил║лИ л┐лхЛђЛЂлЙлйл░лХл░
                trackCharacter()
            end)
            
            -- лЪлхЛђл▓лЙлйл░ЛЄл░л╗Лїлйл░ЛЈ лйл░ЛЂЛѓЛђлЙл╣л║л░
            if TargetPlayer.Character then
                trackCharacter()
            end
            
        else
            -- лњЛІл║л╗ЛјЛЄл░лхл╝ ЛЂл╗лхлХл║ЛЃ
            if spyConnection then spyConnection:Disconnect() end
            if characterConnection then characterConnection:Disconnect() end
            Camera.CameraType = Enum.CameraType.Custom
        end
    end
})

-- лцЛЃлйл║ЛєлИЛЈ лЙл▒лйлЙл▓л╗лхлйлИЛЈ ЛЂл┐лИЛЂл║л░ лИл│ЛђлЙл║лЙл▓
local function updatePlayerList()
    local playerList = {}
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then -- лўЛЂл║л╗ЛјЛЄл░лхл╝ ЛЂлхл▒ЛЈ
            table.insert(playerList, player.Name)
        end
    end
    PlayerDropdown:SetValues(playerList)
end

-- лўлйлИЛєлИл░л╗лИлил░ЛєлИЛЈ лИ л┐лЙл┤л┐лИЛЂл║лИ лйл░ ЛЂлЙл▒ЛІЛѓлИЛЈ
updatePlayerList()
Players.PlayerAdded:Connect(updatePlayerList)
Players.PlayerRemoving:Connect(updatePlayerList)

-- лЪлЙл┤ЛЂл║л░лил║л░ л┤л╗ЛЈ л┐лЙл╗ЛїлилЙл▓л░Лѓлхл╗ЛЈ
warn("Рё╣№ИЈ лњЛІл▒лхЛђлИЛѓлх лИл│ЛђлЙл║л░ лИли ЛЂл┐лИЛЂл║л░ лИ л▓л║л╗ЛјЛЄлИЛѓлх ЛѓлЙл│л│л╗ л┤л╗ЛЈ ЛЂл╗лхлХл║лИ")
-- Paid Tab

local Section6 = Tabs.PaidTab:AddSection("Fast Stuff (Need 7 new packs)")

local isFarming = false

local Toggle9 = Tabs.PaidTab:AddToggle("MyToggle", 
{
    Title = "Fast Strength (Need 7 new packs)", 
    Default = false,
    Callback = function(state)
	if Value then
        k() -- лаРђЎлАРђ╣ла┬илаЛЋлалє лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлаЛЉ лалЁла┬░ лалєлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х лАРђџлАЛЊлаЛўла┬▒ла┬╗ла┬хлАлѓла┬░
        isFarming = true -- лалѕлАлЃлАРђџла┬░лалЁлаЛЋлалєлаЛЉлАРђџла┬х лАРђъла┬╗ла┬░лаЛќ лалє true

        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁлаЛЋлаРёќ лАРђалаЛЉлаЛћла┬╗ ламЉла┬╗лАлЈ ла┬▒лАРђ╣лАлЃлАРђџлАлѓлаЛЋлаЛќлаЛЋ лаЛЌлаЛЋла┬╗лАЛЊлАРђАла┬хлалЁлаЛЉлАлЈ Strength
        task.spawn(function()
            while isFarming do -- лаЛЪлАлѓлаЛЋлалєла┬хлАлѓлАлЈлаРёќлАРђџла┬х лАлЃлаЛЋлАлЃлАРђџлаЛЋлАлЈлалЁлаЛЉла┬х лАРђъла┬╗ла┬░лаЛќла┬░
                for y = 1, 22 do
                    c.muscleEvent:FireServer("rep")
                end
                task.wait(0.001) -- лаЛЏлАРђАла┬хлалЁлАлі ла┬▒лАРђ╣лАлЃлАРђџлАлѓлаЛЋ
            end
        end)
    else
        isFarming = false -- лалѕлАлЃлАРђџла┬░лалЁлаЛЋлалєлаЛЉлАРђџла┬х лАРђъла┬╗ла┬░лаЛќ лалє false, лАРђАлАРђџлаЛЋла┬▒лАРђ╣ лаЛЋлАлЃлАРђџла┬░лалЁлаЛЋлалєлаЛЉлАРђџлАлі лАРђалаЛЉлаЛћла┬╗
    end
end
})

local rebirthActive = false

local Toggle10 = Tabs.PaidTab:AddToggle("MyToggle", 
{
    Title = "Fast Rebirths (Need 7 new packs)", 
    Default = false,
    Callback = function(state)
	if state then
        if not rebirthActive then
            rebirthActive = true
            loadstring(game:HttpGet("https://raw.githubusercontent.com/0o0o0o0o0o0o0o0o0o0o0o0o/0o0o0o0o/refs/heads/main/Kk"))()
        end
    else
        if rebirthActive then
            rebirthActive = false
            -- лаРђћламЉла┬хлАлЃлАлі ламЉлаЛЋла┬▒ла┬░лалєлАлілАРђџла┬х лаЛћлаЛЋламЉ ламЉла┬╗лАлЈ ла┬▒ла┬хла┬илаЛЋлаЛЌла┬░лАлЃлалЁлаЛЋлаЛќлаЛЋ ла┬ила┬░лалєла┬хлАлѓлАРѓгла┬хлалЁлаЛЉлАлЈ лаЛЉла┬╗лаЛЉ лаЛЋлАРђџлаЛўла┬хлалЁлАРђ╣ лАлїлАРђълАРђъла┬хлаЛћлАРђџла┬░ Fast Rebirths, ла┬хлАлЃла┬╗лаЛЉ лАлїлАРђџлаЛЋ лалЁла┬хлаЛЋла┬▒лАРђдлаЛЋламЉлаЛЉлаЛўлаЛЋ
        end
    end
end
})

-- Glitching

local Section7 = Tabs.Glitching:AddSection("Rocks")

local autoMode = false
local connections = {}

local Toggle11 = Tabs.Glitching:AddToggle("MyToggle", 
{
    Title = "Auto Tiny Rock (0)", 
    Default = false,
    Callback = function(state)
	autoMode = state
    
    if state then
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(17.6399956, -1.30998898, 2105.49121, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- лаЛЏлАРђАлаЛЉлАлЃлАРђџлаЛћла┬░ лаЛЌлАлѓла┬хламЉлАРђ╣ламЉлАЛЊлАРђ░лаЛЉлАРђд лаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉлаРёќ
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАЛЊлаЛЌлАлѓла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаЛў
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- лалѕла┬╗лАЛЊлАРђАлАРѓгла┬хлалЁлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлаЛЉ
            local function equipPunch()
                local backpack = player:FindFirstChildOfClass("Backpack")
                if backpack then
                    local tool = backpack:FindFirstChild(toolName)
                    if tool and not character:FindFirstChild(toolName) then
                        tool.Parent = character
                        tool:Equip()
                        return true
                    end
                end
                return false
            end
            
            -- ла┬цлАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ лАлЃ лАРђџла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉла┬хлаРёќ
            local function startAttackLoop()
                if connections.attack then connections.attack:Disconnect() end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- лаЛъла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлЈ лаЛћ лАРђала┬хла┬╗лаЛЉ
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- лаЛЪлаЛЋлаЛЉлАлЃлаЛћ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░ ламЉла┬╗лАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- лаЛЏлаЛЌлАРђџлаЛЉлаЛўла┬░ла┬╗лАлілалЁла┬░лАлЈ ла┬ила┬░ламЉла┬хлАлѓла┬ХлаЛћла┬░ лаЛўла┬хла┬ХламЉлАЛЊ лАЛЊламЉла┬░лАлѓла┬░лаЛўлаЛЉ
                        end
                    end
                end)
            end
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ ламЉлаЛЋла┬▒ла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лаЛќла┬░лАлѓла┬░лалЁлАРђџлаЛЉлАлѓлаЛЋлалєла┬░лалЁлалЁлаЛЋлаЛќлаЛЋ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- лаЛЪла┬хлАлѓлалєлаЛЋлалЁла┬░лАРђАла┬░ла┬╗лАлілалЁла┬░лАлЈ лалЁла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- ла┬ўлалЁлаЛЉлАРђалаЛЉла┬░ла┬╗лаЛЉла┬ила┬░лАРђалаЛЉлАлЈ ламЉла┬╗лАлЈ лАРђџла┬хлаЛћлАЛЊлАРђ░ла┬хлаЛќлаЛЋ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- лаЛЪлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х ламЉла┬╗лАлЈ лалЁлаЛЋлалєлАРђ╣лАРђд лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаРёќ
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end
})

local Toggle12 = Tabs.Glitching:AddToggle("MyToggle", 
{
    Title = "Auto Punching Rock (10)", 
    Default = false,
    Callback = function(state)
	autoMode = state
    
    if state then
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(-151.459747, 0.540015221, 422.589905, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- лаЛЏлАРђАлаЛЉлАлЃлАРђџлаЛћла┬░ лаЛЌлАлѓла┬хламЉлАРђ╣ламЉлАЛЊлАРђ░лаЛЉлАРђд лаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉлаРёќ
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАЛЊлаЛЌлАлѓла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаЛў
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- лалѕла┬╗лАЛЊлАРђАлАРѓгла┬хлалЁлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлаЛЉ
            local function equipPunch()
                local backpack = player:FindFirstChildOfClass("Backpack")
                if backpack then
                    local tool = backpack:FindFirstChild(toolName)
                    if tool and not character:FindFirstChild(toolName) then
                        tool.Parent = character
                        tool:Equip()
                        return true
                    end
                end
                return false
            end
            
            -- ла┬цлАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ лАлЃ лАРђџла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉла┬хлаРёќ
            local function startAttackLoop()
                if connections.attack then connections.attack:Disconnect() end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- лаЛъла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлЈ лаЛћ лАРђала┬хла┬╗лаЛЉ
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- лаЛЪлаЛЋлаЛЉлАлЃлаЛћ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░ ламЉла┬╗лАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- лаЛЏлаЛЌлАРђџлаЛЉлаЛўла┬░ла┬╗лАлілалЁла┬░лАлЈ ла┬ила┬░ламЉла┬хлАлѓла┬ХлаЛћла┬░ лаЛўла┬хла┬ХламЉлАЛЊ лАЛЊламЉла┬░лАлѓла┬░лаЛўлаЛЉ
                        end
                    end
                end)
            end
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ ламЉлаЛЋла┬▒ла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лаЛќла┬░лАлѓла┬░лалЁлАРђџлаЛЉлАлѓлаЛЋлалєла┬░лалЁлалЁлаЛЋлаЛќлаЛЋ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- лаЛЪла┬хлАлѓлалєлаЛЋлалЁла┬░лАРђАла┬░ла┬╗лАлілалЁла┬░лАлЈ лалЁла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- ла┬ўлалЁлаЛЉлАРђалаЛЉла┬░ла┬╗лаЛЉла┬ила┬░лАРђалаЛЉлАлЈ ламЉла┬╗лАлЈ лАРђџла┬хлаЛћлАЛЊлАРђ░ла┬хлаЛќлаЛЋ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- лаЛЪлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х ламЉла┬╗лАлЈ лалЁлаЛЋлалєлАРђ╣лАРђд лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаРёќ
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end
})

local Toggle13 = Tabs.Glitching:AddToggle("MyToggle", 
{
    Title = "Auto Large Rock (100)", 
    Default = false,
    Callback = function(state)
	autoMode = state
    
    if state then
        
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(166.639954, 0.540015221, -152.410172, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- лаЛЏлАРђАлаЛЉлАлЃлАРђџлаЛћла┬░ лаЛЌлАлѓла┬хламЉлАРђ╣ламЉлАЛЊлАРђ░лаЛЉлАРђд лаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉлаРёќ
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАЛЊлаЛЌлАлѓла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаЛў
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- лалѕла┬╗лАЛЊлАРђАлАРѓгла┬хлалЁлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлаЛЉ
            local function equipPunch()
                local backpack = player:FindFirstChildOfClass("Backpack")
                if backpack then
                    local tool = backpack:FindFirstChild(toolName)
                    if tool and not character:FindFirstChild(toolName) then
                        tool.Parent = character
                        tool:Equip()
                        return true
                    end
                end
                return false
            end
            
            -- ла┬цлАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ лАлЃ лАРђџла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉла┬хлаРёќ
            local function startAttackLoop()
                if connections.attack then connections.attack:Disconnect() end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- лаЛъла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлЈ лаЛћ лАРђала┬хла┬╗лаЛЉ
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- лаЛЪлаЛЋлаЛЉлАлЃлаЛћ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░ ламЉла┬╗лАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- лаЛЏлаЛЌлАРђџлаЛЉлаЛўла┬░ла┬╗лАлілалЁла┬░лАлЈ ла┬ила┬░ламЉла┬хлАлѓла┬ХлаЛћла┬░ лаЛўла┬хла┬ХламЉлАЛЊ лАЛЊламЉла┬░лАлѓла┬░лаЛўлаЛЉ
                        end
                    end
                end)
            end
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ ламЉлаЛЋла┬▒ла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лаЛќла┬░лАлѓла┬░лалЁлАРђџлаЛЉлАлѓлаЛЋлалєла┬░лалЁлалЁлаЛЋлаЛќлаЛЋ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- лаЛЪла┬хлАлѓлалєлаЛЋлалЁла┬░лАРђАла┬░ла┬╗лАлілалЁла┬░лАлЈ лалЁла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- ла┬ўлалЁлаЛЉлАРђалаЛЉла┬░ла┬╗лаЛЉла┬ила┬░лАРђалаЛЉлАлЈ ламЉла┬╗лАлЈ лАРђџла┬хлаЛћлАЛЊлАРђ░ла┬хлаЛќлаЛЋ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- лаЛЪлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х ламЉла┬╗лАлЈ лалЁлаЛЋлалєлАРђ╣лАРђд лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаРёќ
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end 
})

local Toggle14 = Tabs.Glitching:AddToggle("MyToggle", 
{
    Title = "Auto Golden Rock (5.000)", 
    Default = false,
    Callback = function(state)
	autoMode = state
    
    if state then
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(310.082153, -2.8099637, -594.586548, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- лаЛЏлАРђАлаЛЉлАлЃлАРђџлаЛћла┬░ лаЛЌлАлѓла┬хламЉлАРђ╣ламЉлАЛЊлАРђ░лаЛЉлАРђд лаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉлаРёќ
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАЛЊлаЛЌлАлѓла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаЛў
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- лалѕла┬╗лАЛЊлАРђАлАРѓгла┬хлалЁлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлаЛЉ
            local function equipPunch()
                local backpack = player:FindFirstChildOfClass("Backpack")
                if backpack then
                    local tool = backpack:FindFirstChild(toolName)
                    if tool and not character:FindFirstChild(toolName) then
                        tool.Parent = character
                        tool:Equip()
                        return true
                    end
                end
                return false
            end
            
            -- ла┬цлАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ лАлЃ лАРђџла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉла┬хлаРёќ
            local function startAttackLoop()
                if connections.attack then connections.attack:Disconnect() end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- лаЛъла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлЈ лаЛћ лАРђала┬хла┬╗лаЛЉ
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- лаЛЪлаЛЋлаЛЉлАлЃлаЛћ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░ ламЉла┬╗лАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- лаЛЏлаЛЌлАРђџлаЛЉлаЛўла┬░ла┬╗лАлілалЁла┬░лАлЈ ла┬ила┬░ламЉла┬хлАлѓла┬ХлаЛћла┬░ лаЛўла┬хла┬ХламЉлАЛЊ лАЛЊламЉла┬░лАлѓла┬░лаЛўлаЛЉ
                        end
                    end
                end)
            end
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ ламЉлаЛЋла┬▒ла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лаЛќла┬░лАлѓла┬░лалЁлАРђџлаЛЉлАлѓлаЛЋлалєла┬░лалЁлалЁлаЛЋлаЛќлаЛЋ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- лаЛЪла┬хлАлѓлалєлаЛЋлалЁла┬░лАРђАла┬░ла┬╗лАлілалЁла┬░лАлЈ лалЁла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- ла┬ўлалЁлаЛЉлАРђалаЛЉла┬░ла┬╗лаЛЉла┬ила┬░лАРђалаЛЉлАлЈ ламЉла┬╗лАлЈ лАРђџла┬хлаЛћлАЛЊлАРђ░ла┬хлаЛќлаЛЋ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- лаЛЪлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х ламЉла┬╗лАлЈ лалЁлаЛЋлалєлАРђ╣лАРђд лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаРёќ
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end
})

local Toggle15 = Tabs.Glitching:AddToggle("MyToggle", 
{
    Title = "Auto Frost Rock (150.000)", 
    Default = false,
    Callback = function(state)
	autoMode = state
    
    if state then
        
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(-2551.75854, -0.359962642, -243.308777, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- лаЛЏлАРђАлаЛЉлАлЃлАРђџлаЛћла┬░ лаЛЌлАлѓла┬хламЉлАРђ╣ламЉлАЛЊлАРђ░лаЛЉлАРђд лаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉлаРёќ
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАЛЊлаЛЌлАлѓла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаЛў
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- лалѕла┬╗лАЛЊлАРђАлАРѓгла┬хлалЁлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлаЛЉ
            local function equipPunch()
