                        -- лаЛъла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлЈ лаЛћ лАРђала┬хла┬╗лаЛЉ
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- лаЛЪлаЛЋлаЛЉлАлЃлаЛћ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░ ламЉла┬╗лАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- лаЛЏлаЛЌлАРђџлаЛЉлаЛўла┬░ла┬╗лАлілалЁла┬░лАлЈ ла┬ила┬░ламЉла┬хлАлѓла┬ХлаЛћла┬░ лаЛўла┬хла┬ХламЉлАЛЊ лАЛЊламЉла┬░лАлѓла┬░лаЛўлаЛЉ
                        end
                    end
                end)
            end
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ ламЉлаЛЋла┬▒ла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лаЛќла┬░лАлѓла┬░лалЁлАРђџлаЛЉлАлѓлаЛЋлалєла┬░лалЁлалЁлаЛЋлаЛќлаЛЋ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- лаЛЪла┬хлАлѓлалєлаЛЋлалЁла┬░лАРђАла┬░ла┬╗лАлілалЁла┬░лАлЈ лалЁла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- ла┬ўлалЁлаЛЉлАРђалаЛЉла┬░ла┬╗лаЛЉла┬ила┬░лАРђалаЛЉлАлЈ ламЉла┬╗лАлЈ лАРђџла┬хлаЛћлАЛЊлАРђ░ла┬хлаЛќлаЛЋ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- лаЛЪлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х ламЉла┬╗лАлЈ лалЁлаЛЋлалєлАРђ╣лАРђд лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаРёќ
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end
})

local Toggle17 = Tabs.Glitching:AddToggle("MyToggle", 
{
    Title = "Auto Eternal Rock (750.000)", 
    Default = false,
    Callback = function(state)
	autoMode = state
    
    if state then
        
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(-7262.18701, -0.359961987, -1259.24426, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        
        -- лаЛЏлАРђАлаЛЉлАлЃлАРђџлаЛћла┬░ лаЛЌлАлѓла┬хламЉлАРђ╣ламЉлАЛЊлАРђ░лаЛЉлАРђд лаЛЌлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉлаРёќ
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- лаЛЏлАлЃлалЁлаЛЋлалєлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАЛЊлаЛЌлАлѓла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаЛў
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- лалѕла┬╗лАЛЊлАРђАлАРѓгла┬хлалЁлалЁла┬░лАлЈ лАРђълАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ лАлїлаЛћлаЛЉлаЛЌлаЛЉлАлѓлаЛЋлалєлаЛћлаЛЉ
            local function equipPunch()
                local backpack = player:FindFirstChildOfClass("Backpack")
                if backpack then
                    local tool = backpack:FindFirstChild(toolName)
                    if tool and not character:FindFirstChild(toolName) then
                        tool.Parent = character
                        tool:Equip()
                        return true
                    end
                end
                return false
            end
            
            -- ла┬цлАЛЊлалЁлаЛћлАРђалаЛЉлАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ лАлЃ лАРђџла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉла┬хлаРёќ
            local function startAttackLoop()
                if connections.attack then connections.attack:Disconnect() end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- лаЛъла┬хла┬╗ла┬хлаЛЌлаЛЋлАлѓлАРђџла┬░лАРђалаЛЉлАлЈ лаЛћ лАРђала┬хла┬╗лаЛЉ
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- лаЛЪлаЛЋлаЛЉлАлЃлаЛћ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░ ламЉла┬╗лАлЈ ла┬░лАРђџла┬░лаЛћлаЛЉ
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- лаЛЏлаЛЌлАРђџлаЛЉлаЛўла┬░ла┬╗лАлілалЁла┬░лАлЈ ла┬ила┬░ламЉла┬хлАлѓла┬ХлаЛћла┬░ лаЛўла┬хла┬ХламЉлАЛЊ лАЛЊламЉла┬░лАлѓла┬░лаЛўлаЛЉ
                        end
                    end
                end)
            end
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ ламЉлаЛЋла┬▒ла┬░лалєла┬╗ла┬хлалЁлаЛЉлАлЈ лаЛЉлалЁлАлЃлАРђџлАлѓлАЛЊлаЛўла┬хлалЁлАРђџла┬░
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- лаЛЏла┬▒лАлѓла┬░ла┬▒лаЛЋлАРђџлАРђАлаЛЉлаЛћ лАлЃлаЛўла┬хлАлѓлАРђџлаЛЉ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- лалѕлалєла┬хла┬╗лаЛЉлАРђАла┬хлалЁлалЁлаЛЋла┬х лалєлАлѓла┬хлаЛўлАлЈ ламЉла┬╗лАлЈ лаЛќла┬░лАлѓла┬░лалЁлАРђџлаЛЉлАлѓлаЛЋлалєла┬░лалЁлалЁлаЛЋлаЛќлаЛЋ лалєлаЛЋла┬илАлѓлаЛЋла┬ХламЉла┬хлалЁлаЛЉлАлЈ
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- лаЛЪла┬хлАлѓлалєлаЛЋлалЁла┬░лАРђАла┬░ла┬╗лАлілалЁла┬░лАлЈ лалЁла┬░лАлЃлАРђџлАлѓлаЛЋлаРёќлаЛћла┬░
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- ла┬ўлалЁлаЛЉлАРђалаЛЉла┬░ла┬╗лаЛЉла┬ила┬░лАРђалаЛЉлАлЈ ламЉла┬╗лАлЈ лАРђџла┬хлаЛћлАЛЊлАРђ░ла┬хлаЛќлаЛЋ лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬░
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- лаЛЪлаЛЋламЉлаЛћла┬╗лАлІлАРђАла┬хлалЁлаЛЉла┬х ламЉла┬╗лАлЈ лалЁлаЛЋлалєлАРђ╣лАРђд лаЛЌла┬хлАлѓлАлЃлаЛЋлалЁла┬░ла┬Хла┬хлаРёќ
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end
})

