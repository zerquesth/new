            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- РЈРІРµР»РёС‡РµРЅРЅРѕРµ РІСЂРµРјСЏ РґР»СЏ РіР°СЂР°РЅС‚РёСЂРѕРІР°РЅРЅРѕРіРѕ РІРѕР·СЂРѕР¶РґРµРЅРёСЏ
                if autoMode and player.Character then
                    equipPunch()
                end
